{
    "manifest_version": 3,
    "name": "Playlist Search",
    "version": "1.0",
    "description": "",
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://www.youtube.com/*"
            ]
        }
    ],
    "icons": {
        "16":"images/16.png",
        "32":"images/32.png",
        "48":"images/48.png",
        "128":"images/128.png"
    }
}